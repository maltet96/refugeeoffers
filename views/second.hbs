{{> head}}
    <link rel="stylesheet" href='../stylesheets/style.css'>
    {{> titleandlang}}
    <div class="categories">
      {{#each categories}}
      <a href='/third/{{this.firstLevel}}/{{this.id}}?lang={{chosenLang}}'>
          <div class='category'>            
            <h2 class='name'>{{this.name}}</h2>
            {{{this.icon}}}
          </div>
       </a>
      {{/each}}
    </div>
{{> foot}}